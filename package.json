{
  "name": "githubclient",
  "version": "1.0.0",
  "description": "github client to search github users",
  "main": "server.js",
  "scripts": {
    "dev": "concurrently -k \"lite-server\" \"npm run watch-css\"",
    "start": "node server.js",
    "build-css": "node-sass --include-path scss scss/main.scss   client/asset/main.css",
    "watch-css": "nodemon -e scss -x \"npm run build-css\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/madhankumar028/githubclient.git"
  },
  "keywords": [],
  "author": "MadhankumarJ",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/madhankumar028/githubclient/issues"
  },
  "homepage": "https://github.com/madhankumar028/githubclient#readme",
  "dependencies": {
    "express": "latest"
  },
  "devDependencies": {
    "concurrently": "^3.4.0",
    "express": "latest",
    "lite-server": "^2.2.2",
    "node-sass": "^4.5.0",
    "nodemon": "^1.11.0"
  }
}
